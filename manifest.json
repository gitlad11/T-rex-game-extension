{
	"manifest_version" : 2,
	"name" : "Chrome features",
	"version" : "1.0",
	"description" : "Chrome new features for browser with youtube popup",

	"permissions" : [
		"storage",
		"<all_urls>"
	],
	"browser_action" : {
		"default_icon" : "browser.png",
		"default_popup" : "dinoPopup.html"
	},
	"commands" : {
		"_execute_browser_action" : {
			"suggested_key" : {
				"default" : "Ctrl +Shift + E",
				"windows" : "Alt + Shift + E",
				"linux" : "Alt + Shift + E"
			}
		}
	}
}